{
  "name": "birdsol/respond",
  "description": "A set of helper api response classes implementing the responsable interface",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mahmoud Elsayed",
      "email": "mahmoud@birdsol.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "laravel/framework": "~5.5.0|~5.6.0|~5.7.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0",
    "phpunit/phpunit": "^6.2|^7.0"
  },
  "autoload": {
    "psr-4": {
      "BirdSolutions\\Respond\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "BirdSolutions\\Respond\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "BirdSolutions\\Respond\\Providers\\RespondServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
